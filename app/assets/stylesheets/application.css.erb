  width: 110;
  display: inline-block;
  vertical-align: top;
  white-space: normal;
  word-wrap: normal;
  font-size: 12px;
  line-height: 1.2;
  margin: 0;
  padding: 2px;
  border: 1px solid #d0a600
}
ul.boxline li span{
  display: block;
  width: 100;
  word-break: break-all;
}

ul.boxline li img {
  display: block!important;
  border: 1px solid #d0a600!important;
